\end_layout

\begin_layout Enumerate
Ailerons: Travel -30° up, 17° down.
 
\end_layout

\begin_layout Enumerate
Flaps: Travel -45 down.
 
\end_layout

\begin_layout Enumerate
Elevator: -30° up and 25° down.
\end_layout

\begin_layout Enumerate
